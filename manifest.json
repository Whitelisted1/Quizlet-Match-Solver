{
    "manifest_version": 2,
    "name": "Quizlet Match Completion",
    "description": "Automatically completes quizlet matching for you",
    "version": "0.0.1",
    "icons": {
    },
    
    "content_scripts": [
      {
        "matches": ["*://quizlet.com/*"],
        "js": ["background.js"],
        "css": ["quizletMatch.css"]
      }
    ],

    "browser_action": {
      "default_popup": "options.html"
    },
    "options_ui": {
      "page": "options.html"
    },

    "permissions": [
        "<all_urls>"
    ]
  }